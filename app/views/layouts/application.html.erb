<!DOCTYPE html>
<html>
<head>
  <title>FootballTube</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrapper">
    <%= render "shared/header" %>
    <div id="main">
      <div class="alert"></div>
      <div id="content">
        <%= yield %>
      </div>
    </div>
  </div>
  <%= render "shared/header" %>
</body>
</html>
