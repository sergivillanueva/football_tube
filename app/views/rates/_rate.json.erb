{
  "id": <%= rate.rateable.id %>,
  "home_team": <%= raw "#{rate.rateable.home_team.name} #{rate.rateable.home_team.decorate.logo("mini")}".to_json %>,
  "result": <%= raw link_to(rate.rateable.decorate.result, rate.rateable, target: "_blank").to_json %>,
  "away_team": <%= raw "#{rate.rateable.away_team.name} #{rate.rateable.away_team.decorate.logo("mini")}".to_json %>,
  "rater": "<%= rate.rater.email %>",
  "stars": <%= rate.stars %>,
  "created_at": "<%= l rate.created_at, format: :long %>"
}
