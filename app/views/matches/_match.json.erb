{
  "home_team": <%= raw "#{match.object.home_team.name} #{match.object.home_team.decorate.logo("mini")}".to_json %>,
  "result": <%= raw link_to(match.result, match, title: "view").to_json %>,
  "away_team": <%= raw "#{match.object.away_team.name} #{match.object.away_team.decorate.logo("mini")}".to_json %>,
  "competition": "<%= match.competition_name %>",
  "stage": "<%= match.stage %>",
  "playing_date": "<%= match.playing_date %>",
  "language": "<%= match.language %>",
  "videos_count": <%= raw match.videos.count > 0 ? match.videos.count : content_tag(:span, "Not found", { class: "text-danger" }).to_json %>,
  "visits_counter": <%= match.visits_counter %>,
  "visualizations_counter": <%= match.visualizations_counter %>,
  "created_at": "<%= match.created_at %>",
  "user": "<%= match.object.user.try(:email) %>",
  "publish_link": <%= raw match.object.published? ? "".to_json : link_to('', "#", class: 'ui-icon ui-icon-check publish', title: 'publish', :"data-url" => "#{publish_match_path(match)}").to_json %>,
  "edit_link": <%= raw link_to('', edit_match_path(match), class: 'ui-icon ui-icon-pencil', title: 'edit').to_json %>,
  "delete_link": <%= raw link_to('', match_path(match), method: :delete, class: 'ui-icon ui-icon-trash', :"data-confirm" => "Are you sure?", title: 'delete').to_json %>
}
