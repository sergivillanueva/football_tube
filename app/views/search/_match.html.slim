tr
  td.playing_date.hidden-xs
    = match.playing_date
  td.match
    .hidden-xs
      = link_to match.full_title, match
    .hidden-md.hidden-lg
      = link_to match.title, match
  td.competition
    = match.competition_name
  td.season.hidden-xs
    = match.season
  td.language.hidden-xs
    = match.language
    
